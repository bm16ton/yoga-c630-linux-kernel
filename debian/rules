#!/usr/bin/make -f

srctree ?= .

build-indep:
build-arch:
	$(MAKE) KERNELRELEASE=6.1.8-16ton ARCH=arm64 	KBUILD_BUILD_VERSION=59 -f $(srctree)/Makefile

build: build-arch

binary-indep:
binary-arch: build-arch
	$(MAKE) KERNELRELEASE=6.1.8-16ton ARCH=arm64 	KBUILD_BUILD_VERSION=59 -f $(srctree)/Makefile intdeb-pkg

clean:
	rm -rf debian/*tmp debian/files
	$(MAKE) clean

binary: binary-arch
