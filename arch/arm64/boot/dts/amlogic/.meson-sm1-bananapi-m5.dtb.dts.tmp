   hiu: bus@3c000 {
    compatible = "simple-bus";
    reg = <0x0 0x3c000 0x0 0x1400>;
    #address-cells = <2>;
    #size-cells = <2>;
    ranges = <0x0 0x0 0x0 0x3c000 0x0 0x1400>;

    hhi: system-controller@0 {
     compatible = "amlogic,meson-gx-hhi-sysctrl",
           "simple-mfd", "syscon";
     reg = <0 0 0 0x400>;

     clkc: clock-controller {
      compatible = "amlogic,g12a-clkc";
      #clock-cells = <1>;
      clocks = <&xtal>;
      clock-names = "xtal";
     };

     pwrc: power-controller {
      compatible = "amlogic,meson-g12a-pwrc";
      #power-domain-cells = <1>;
      amlogic,ao-sysctrl = <&rti>;
      resets = <&reset 5>,
        <&reset 10>,
        <&reset 13>,
        <&reset 37>,
        <&reset 133>,
        <&reset 134>,
        <&reset 135>,
        <&reset 137>,
        <&reset 140>,
        <&reset 141>,
        <&reset 231>;
      reset-names = "viu", "venc", "vcbus", "bt656",
             "rdma", "venci", "vencp", "vdac",
             "vdi6", "vencl", "vid_lock";
      clocks = <&clkc 116>,
        <&clkc 124>;
      clock-names = "vpu", "vapb";






      assigned-clocks = <&clkc 110>,
          <&clkc 112>,
          <&clkc 116>,
          <&clkc 117>,
          <&clkc 119>,
          <&clkc 123>;
      assigned-clock-parents = <&clkc 3>,
          <0>,
          <&clkc 112>,
          <&clkc 4>,
          <0>,
          <&clkc 119>;
      assigned-clock-rates = <0>,
               <666666666>,
               <0>,
               <0>,
               <250000000>,
               <0>;
     };
    };
   };

