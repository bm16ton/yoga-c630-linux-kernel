 non-removable;
 disable-wp;
 mmc-ddr-1_8v;
 mmc-hs200-1_8v;

 mmc-pwrseq = <&emmc_pwrseq>;
 vmmc-supply = <&vcc_3v3>;
 vqmmc-supply = <&vddio_boot>;
};


&uart_A {
 status = "okay";
 pinctrl-0 = <&uart_a_pins>, <&uart_a_cts_rts_pins>;
 pinctrl-names = "default";

 bluetooth {
  compatible = "brcm,bcm43438-bt";
  shutdown-gpios = <&gpio 112 0>;
  max-speed = <2000000>;
  clocks = <&wifi32k>;
  clock-names = "lpo";
 };
