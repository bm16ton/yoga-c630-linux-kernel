};

&rtc {
 clocks = <&ext_osc32k>;
};







&spi0 {
 pinctrl-0 = <&spi0_pins>, <&spi0_cs_pin>;
 pinctrl-names = "default";
 status = "disabled";

 flash@0 {
  compatible = "winbond,w25q128", "jedec,spi-nor";
  reg = <0>;
  spi-max-frequency = <4000000>;
 };
};

&uart0 {
 pinctrl-names = "default";
 pinctrl-0 = <&uart0_ph_pins>;
 status = "okay";
};

&usb2otg {
 dr_mode = "host";
 status = "okay";
};

&usb2phy {
 usb0_vbus-supply = <&reg_usb_vbus>;
 usb3_vbus-supply = <&reg_usb_vbus>;
 status = "okay";
};
