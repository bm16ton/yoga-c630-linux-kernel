   reg = <0x05002800 0x400>;
   interrupts = <0 8 4>;
   clocks = <&ccu 74>;
   resets = <&ccu 25>;
   status = "disabled";
   #address-cells = <1>;
   #size-cells = <0>;
  };

  i2c3: i2c@5002c00 {
   compatible = "allwinner,sun50i-h616-i2c",
         "allwinner,sun8i-v536-i2c",
         "allwinner,sun6i-a31-i2c";
   reg = <0x05002c00 0x400>;
   interrupts = <0 9 4>;
   clocks = <&ccu 75>;
   resets = <&ccu 26>;
   status = "disabled";
   #address-cells = <1>;
   #size-cells = <0>;
  };

  i2c4: i2c@5003000 {
