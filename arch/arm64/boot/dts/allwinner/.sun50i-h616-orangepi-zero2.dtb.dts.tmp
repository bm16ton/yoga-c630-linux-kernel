   reg = <0x05002400 0x400>;
   interrupts = <0 7 4>;
   clocks = <&ccu 73>;
   resets = <&ccu 24>;
   status = "disabled";
   #address-cells = <1>;
   #size-cells = <0>;
  };

  i2c2: i2c@5002800 {
