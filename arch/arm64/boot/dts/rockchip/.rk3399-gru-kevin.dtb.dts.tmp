};

&gpio_keys {
 pinctrl-names = "default";
 pinctrl-0 = <&bt_host_wake_l>, <&cpu1_pen_eject>;

 switch-pen-insert {
  label = "Pen Insert";

  gpios = <&gpio0 13 1>;
  linux,code = <0x0f>;
  linux,input-type = <0x05>;
  wakeup-source;
 };
};

&thermal_zones {
 bigcpu_reg_thermal: bigcpu-reg-thermal {
  polling-delay-passive = <100>;
  polling-delay = <1000>;
  thermal-sensors = <&thermistor_ppvar_bigcpu 0>;
  sustainable-power = <4000>;

  ppvar_bigcpu_trips: trips {
   ppvar_bigcpu_on: ppvar-bigcpu-on {
    temperature = <40000>;
    hysteresis = <2000>;
    type = "passive";
   };

   ppvar_bigcpu_alert: ppvar-bigcpu-alert {
    temperature = <50000>;
    hysteresis = <2000>;
    type = "passive";
   };

   ppvar_bigcpu_crit: ppvar-bigcpu-crit {
    temperature = <90000>;
    hysteresis = <0>;
    type = "critical";
   };
  };

  cooling-maps {
   map0 {
    trip = <&ppvar_bigcpu_alert>;
    cooling-device =
     <&cpu_l0 (~0) (~0)>,
     <&cpu_l1 (~0) (~0)>,
     <&cpu_l2 (~0) (~0)>,
     <&cpu_l3 (~0) (~0)>;
    contribution = <4096>;
   };
   map1 {
    trip = <&ppvar_bigcpu_alert>;
    cooling-device =
     <&cpu_b0 (~0) (~0)>,
     <&cpu_b1 (~0) (~0)>;
    contribution = <1024>;
   };
  };
 };

 litcpu_reg_thermal: litcpu-reg-thermal {
  polling-delay-passive = <100>;
  polling-delay = <1000>;
  thermal-sensors = <&thermistor_ppvar_litcpu 0>;
  sustainable-power = <4000>;

  ppvar_litcpu_trips: trips {
   ppvar_litcpu_on: ppvar-litcpu-on {
    temperature = <40000>;
    hysteresis = <2000>;
    type = "passive";
   };

   ppvar_litcpu_alert: ppvar-litcpu-alert {
    temperature = <50000>;
    hysteresis = <2000>;
    type = "passive";
   };

   ppvar_litcpu_crit: ppvar-litcpu-crit {
    temperature = <90000>;
    hysteresis = <0>;
    type = "critical";
   };
  };
 };
};

