  media2_crg: media2_crgctrl@e8900000 {
   compatible = "hisilicon,hi3670-media2-crg","syscon";
   reg = <0x0 0xe8900000 0x0 0x1000>;
   #clock-cells = <1>;
  };

  iomcu_rst: reset {
   compatible = "hisilicon,hi3660-reset";
   hisi,rst-syscon = <&iomcu>;
   #reset-cells = <2>;
  };

